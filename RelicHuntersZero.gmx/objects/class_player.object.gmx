    draw_sprite_part( sprite_index,image_index,0,0,sprite_width,sprite_height-digDepth,x-sprite_xoffset,(y-sprite_yoffset)+digDepth );
   
    if (animation_current != 'dig') draw_sprite(spr_shovel_idle,image_index,x,y);
